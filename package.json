    "jquery-i18next": "^1.2.1",
    "ol": "^10.7.0",
    "ol-mapbox-style": "^13.1.1",
    "papaparse": "^5.4.1",
    "simple-statistics": "^7.8.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^16.3.1",
    "jsdom": "^23.0.1",
    "knip": "^5.62.0",
    "replace-in-file": "^7.0.2",
    "rollup-plugin-visualizer": "^5.11.0",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",
    "vite-plugin-cp": "^4.0.1",
    "vitest": "^3.2.4"
  },
  "knip": {
    "entry": [
      "./js/index.js"
