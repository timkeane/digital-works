{
  "name": "digital-empowerment",
  "version": "2.2.7",
  "type": "module",
  "scripts": {
    "start": "APP_ENV=dev vite --host",
    "build": "APP_ENV=prd vite build",
    "build:debug": "APP_ENV=dev vite build",
    "prebuild": "rm -rf ./dist",
    "postbuild": "APP_ENV=prd node ./post-build.js",
